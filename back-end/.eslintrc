{
  "extends": ["airbnb-base", "prettier"],
  "rules": {
    "no-underscore-dangle": ["error", { "allow": ["_id", "_doc"] }],
    "implicit-arrow-linebreak": "off",
    "no-useless-escape": "off", //For our regex to not interfere with our program
    "no-unused-vars": [
      "error",
      { "argsIgnorePattern": "next", "vars": "password" }
    ],
    "eqeqeq": "off"
  }
}
